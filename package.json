{
  "name": "cpaas-management",
  "productName": "CPaaS Management",
  "version": "1.0.0",
  "description": "Financial ledger and margin tracking system for CPaaS operations",
  "main": "dist-server/index.js",
  "private": true,
  "scripts": {
    "dev:web": "vite --config vite.renderer.config.ts",
    "build:web": "vite build --config vite.renderer.config.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "npm run build:web && npm run build:server",
    "start": "node dist-server/index.js",
    "lint": "eslint --ext .ts,.tsx src/",
    "pretest": "npm rebuild better-sqlite3",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "cpaas",
    "finance",
    "ledger",
    "electron"
  ],
  "author": {
    "name": "Jiwateshwar Singh",
    "email": "jiwateshwar@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/node": "^22.10.10",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.0",
    "vite": "^5.4.21",
    "vitest": "^3.2.4"
  },
